{
  "kind": "collectionType",
  "collectionName": "difficulties",
  "info": {
    "singularName": "difficulty",
    "pluralName": "difficulties",
    "displayName": "Difficulty"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "level": {
      "type": "enumeration",
      "enum": [
        "easy",
        "medium",
        "hard"
      ],
      "default": "easy",
      "required": true
    },
    "weight": {
      "type": "integer"
    },
    "decks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::deck.deck",
      "mappedBy": "difficulty"
    },
    "questions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::question.question",
      "mappedBy": "difficulty"
    }
  }
}
